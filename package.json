{
  "name": "ensime-client",
  "main": "release/js/lib/index.js",
  "typings": "release/definitions/lib/index.d.ts",
  "version": "0.10.0",
  "description": "Ensime integration for node.js",
  "scripts": {
    "prepublish": "gulp build",
    "test": "jasmine JASMINE_CONFIG_PATH=jasmine.json src/spec/*",
    "test:it": "jasmine JASMINE_CONFIG_PATH=jasmine.json src/spec-integration/*",
    "test:all": "jasmine JASMINE_CONFIG_PATH=jasmine.json",
    "test:debug": "node --debug-brk --inspect node_modules/jasmine/bin/jasmine.js JASMINE_CONFIG_PATH=jasmine.json",
    "test:coverage": "node node_modules/istanbul/lib/cli.js cover -e .ts  -x \"*.d.ts\" -x \"*.spec.ts\" node_modules/jasmine/bin/jasmine.js -- JASMINE_CONFIG_PATH=jasmine.json",
    "lint": "tslint --format stylish --force src/**/*.ts",
    "compile": "gulp compile",
    "watch": "gulp watch",
    "clean": "rimraf ./release"
  },
  "repository": "https://github.com/ensime/ensime-node",
  "license": "MIT",
  "dependencies": {
    "chokidar": "1.7.0",
    "fs-extra": "3.0.1",
    "glob": "7.1.2",
    "lodash": "4.17.4",
    "loglevel": "1.4.1",
    "temp": "0.8.3",
    "ws": "3.0.0"
  },
  "devDependencies": {
    "@types/chokidar": "1.7.0",
    "@types/fs-extra": "3.0.3",
    "@types/glob": "5.0.30",
    "@types/jasmine": "2.5.51",
    "@types/lodash": "4.14.65",
    "@types/loglevel": "1.4.29",
    "@types/node": "7.0.29",
    "@types/temp": "0.8.29",
    "@types/ws": "3.0.0",
    "assert": "1.4.1",
    "gulp": "3.9.1",
    "gulp-sourcemaps": "2.6.4",
    "gulp-typescript": "3.1.7",
    "istanbul": "1.1.0-alpha.1",
    "jasmine": "2.6.0",
    "jasmine-focused": "1.0.7",
    "jasmine-spec-reporter": "4.1.0",
    "jasmine-ts": "0.1.3",
    "merge2": "1.0.3",
    "rimraf": "2.6.1",
    "run-sequence": "1.2.2",
    "ts-node": "3.0.6",
    "tslint": "5.4.3",
    "typescript": "2.3.4"
  },
  "engines": {
    "node": ">=6"
  }
}
